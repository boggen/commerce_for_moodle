<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/oc/db" VERSION="20150104" COMMENT="XMLDB file for Moodle local/oc"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
	  <TABLE NAME="storeinfo_country" COMMENT="storeinfo_country">
		  <FIELDS>
			  <FIELD NAME="country_id" TYPE="int" LENGTH="9" NOTNULL="true" SEQUENCE="true"/>
			  <FIELD NAME="name" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false"/>
			  <FIELD NAME="iso_code_2" TYPE="char" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
			  <FIELD NAME="iso_code_3" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
			  <FIELD NAME="address_format" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
			  <FIELD NAME="postcode_required" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
			  <FIELD NAME="status" TYPE="int" LENGTH="2" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
		  </FIELDS>
		  <KEYS>
			  <KEY NAME="primary" TYPE="primary" FIELDS="country_id"/>
		  </KEYS>
	  </TABLE>  
  </TABLES>
</XMLDB>